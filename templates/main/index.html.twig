                        </select>
                      

                    </div>

                  </div>
                  <div class="col-sm-4">
                    <div class="form-group">
                      <button type="submit" class="buttons button-2 search btn btn-primary w-100" style="height: 58px;">
                        <i class="fas fa-search"></i>
                        Search </button>
                    </div>
                  </div>
                </div>
              </form>
            </div>
            {% endfor %}


            
             
          </div>

        </div>
      </div>
    </div>

  </div>


  <div class="bloc-video" data-aos="fade-up"  data-aos-duration="1500">
