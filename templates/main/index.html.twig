{% extends 'base.html.twig' %}

{% block title %}AFRK50{% endblock %}


{% block body %}
  

<style>
.select2-container .select2-selection--single {
    height: 58px !important;
    margin-bottom: -26px !important;
    padding-top: 10px !important;
}
</style>

<header class="main-header">
  <div class="overlay"></div> 
  <div class="video-container">
    <video autoplay muted loop poster="/img/afrk50-header-background.jpg">
      <source src="/img/AFRK50-Intro.mp4" type="video/mp4">
    </video>
  </div>



  {% if message != null %}
    <div class="toast show" role="alert" aria-live="assertive" aria-atomic="true" style="position: fixed;
    z-index: 10000000000;
    top: 15px;
    left: 15px;">
      <div class="toast-header bg-success">
        <img width="39px" src="/img/logo.png" class="rounded me-2" alt="...">
        <strong class="me-auto text-white">AFRK50</strong>
        <small class="text-white">just now</small>
        <button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
      </div>
      <div class="toast-body text-success">
        {{message}}
      </div>
    </div>
  {% endif %}

 

  {% if app.user != null %}
  
    {% if app.user.isLeagalEntity == false %}
            {% if app.user.proInfo == null and app.user.jobSeekerInfo == null and app.user.studentInfo == null   %}
    
