          <div class="card">
            <div class="card-body tab-content tab-content-0 show">
              <form method="GET" action="/explore">
                <div class="row">
                  <div class="col-sm-6 mb-4">
                    <div class="form-group explore-filter wp-search-filter md-group ">
                      
                        <input type="text"  style="height:58px" class="form-control" placeholder="What are you looking for?" name="search"  >
                      

                    </div>

                  </div>
                  <div class="col-sm-6 mb-4">
                    <div class="form-group">
                      <button type="submit" class="buttons button-2 search btn btn-primary w-100" style="height: 58px;">
                        <i class="fas fa-search"></i>
                        Search </button>
                    </div>
                  </div>
                </div>
              </form>
            </div>


            {% for item in listings %}
              <div class="card-body tab-content tab-content-{{loop.index}}">
              <form method="GET"  action="/explore">
                <input type="hidden" name="listing-type" value="{{item.id}}" />
                <div class="row">
                  <div class="col-sm-4 mb-3">
                    <div class="form-group explore-filter wp-search-filter md-group">
                      
                        <input type="text" style="height:58px" class="form-control" placeholder="What are you looking for?" name="search"  >
                      
                       

                    </div>

                  </div>
                  <div class="col-sm-4 mb-3" style="min-height:54px">
