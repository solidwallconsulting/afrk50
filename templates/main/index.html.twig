            <div class="nav-item d-inline-block">
              <a class="nav-link main-link active" tab-target="0" href="#"><i class="fas fa-home"></i> All</a>
            </div>
             


            {% for item in listings %}
              <div class="nav-item d-inline-block">
                <a class="nav-link main-link " tab-target="{{loop.index}}">
                  <i class="{{item.icon}}"></i>
                  {{item.name}}
                </a>
              </div>
            {% endfor %}
            

          </div>
