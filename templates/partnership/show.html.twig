 
{% extends 'admin-base.html.twig' %}


{% block body %}


	<main class="main-content">
		{% block topNav %}
			{{ parent() }}
		{% endblock %}

		<div class="conatiner-fluid content-inner mt-n5 py-0">
			<div class="row">

				<div class="col-12">
					<div class="card">
						<div class="card-header d-flex justify-content-between flex-wrap">
							<div class="header-title">
								<h4 class="card-title">Partnerships request </h4>
							</div>

                            <a href="{{ path('partnership_index') }}" class="btn btn-primary">Back</a>


						</div>
						<div class="card-body w-100">

                            <h1>Partnership</h1>

                            <table class="table">
                                <tbody>
                                    <tr>
                                        <th>Id</th>
                                        <td>{{ partnership.id }}</td>
                                    </tr>
                                    <tr>
                                        <th>Name</th>
                                        <td>{{ partnership.name }}</td>
                                    </tr>
                                    <tr>
                                        <th>Lastname</th>
                                        <td>{{ partnership.lastname }}</td>
                                    </tr>
                                    <tr>
                                        <th>CompanyName</th>
                                        <td>{{ partnership.companyName }}</td>
                                    </tr>
                                    <tr>
                                        <th>Email</th>
                                        <td>{{ partnership.email }}</td>
                                    </tr> 
                                </tbody>
                            </table>
                        

                            <hr>


                            <div class="form-group">
                                <label for="">Description</label>

                                <p>
                                    {{ partnership.serviceDescription }}
                                </p>
                            </div>

 
                        
                            {{ include('partnership/_delete_form.html.twig') }}			 

						</div>


					</div>

				</div>


			{% endblock %}
